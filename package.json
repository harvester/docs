{
  "name": "harvester-docs",
  "version": "0.1.0",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "build-packages": "lerna run build --no-private --stream",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "check-links": "remark --quiet --use remark-validate-links --use remark-lint-no-dead-urls ./docs ./versioned_docs ./i18n",
    "gen-api-docs": "docusaurus gen-api-docs",
    "clean-api-docs": "docusaurus clean-api-docs"
  },
  "dependencies": {
    "@docusaurus/core": "^2.4.1",
    "@docusaurus/plugin-client-redirects": "^2.4.1",
    "@docusaurus/plugin-content-blog": "^2.4.1",
    "@docusaurus/plugin-content-docs": "^2.4.1",
    "@docusaurus/plugin-content-pages": "^2.4.1",
    "@docusaurus/plugin-debug": "^2.4.1",
    "@docusaurus/preset-classic": "^2.4.1",
    "@mdx-js/react": "^1.6.22",
    "@types/jest": "^27.0.2",
    "clsx": "^1.1.1",
    "docusaurus-plugin-image-zoom": "^1.0.0",
    "docusaurus-plugin-openapi-docs": "^2.0.0-beta.3",
    "docusaurus-theme-openapi-docs": "^2.0.0-beta.3",
    "jest": "^27.3.1",
    "lerna": "^5.5.2",
    "prettier": "^2.4.1",
    "prism-react-renderer": "^1.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remark-cli": "^11.0.0",
    "remark-lint-no-dead-urls": "^1.1.0",
    "remark-validate-links": "^12.1.1",
    "typescript": "^4.4.4"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "remarkConfig": {
    "plugins": [
      "remark-validate-links",
      "remark-lint-no-dead-urls"
    ]
  }
}
